{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development",
        "EXPO_PUBLIC_SUPABASE_URL": "https://usjarubftgzhxgbdfspl.supabase.co",
        "EXPO_PUBLIC_API_BASE_URL": "https://squares-api.onrender.com",
        "SENTRY_ORG": "squares-cp",
        "SENTRY_PROJECT": "react-native"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "preview",
        "EXPO_PUBLIC_SUPABASE_URL": "https://usjarubftgzhxgbdfspl.supabase.co",
        "EXPO_PUBLIC_API_BASE_URL": "https://squares-api.onrender.com",
        "SENTRY_ORG": "squares-cp",
        "SENTRY_PROJECT": "react-native"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "distribution": "store",
        "cache": {
          "disabled": true
        }
      },
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_SUPABASE_URL": "https://usjarubftgzhxgbdfspl.supabase.co",
        "EXPO_PUBLIC_API_BASE_URL": "https://squares-api.onrender.com",
        "SENTRY_ORG": "squares-cp",
        "SENTRY_PROJECT": "react-native"
      }
    },
    "test": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "test",
        "EXPO_PUBLIC_SUPABASE_URL": "https://usjarubftgzhxgbdfspl.supabase.co",
        "EXPO_PUBLIC_API_BASE_URL": "https://squares-api.onrender.com",
        "SENTRY_ORG": "squares-cp",
        "SENTRY_PROJECT": "react-native"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "jbkulzer@gmail.com"
      }
    }
  }
}